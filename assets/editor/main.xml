<?xml version="1.0" encoding="utf-8" ?>
<vbox width="100%" height="100%">
    <menubar>
        <menubutton text="File" id="menuFile">
            <menu>
				<menuitem text="New" id="newFile" />
                <menuitem text="Save" id="saveFile" />
                <menuitem text="Close" id="closeFile" />
            </menu>
			
        </menubutton>
		
		<menubutton text="Edit" id="menuEdit">
			<menu>
				<menuitem text="Undo" id="UndoEdit" />
                <menuitem text="Redo" id="RedoEdit" />
            </menu>
		</menubutton>
		
        <spacer width="25"/>
        <button text="play" id="play/pause" toggle="true"/>   
        <button text="stop" id="stop" /> 
    </menubar>
    <hbox width="100%" height="100%">
		<import resource="editor/AssetsBrowser.xml" />
        <Tabview id="editors" width="85%" height="100%">       
        </Tabview>   
    </hbox>
</vbox>